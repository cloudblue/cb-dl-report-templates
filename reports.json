{
   "name":"Data Lake Reports",
   "readme_file":"README.md",
   "version":"0.1.0",
   "language":"python",
   "reports":[
      {
         "name":"Approved Requests",
         "readme_file":"reports/approved_requests/README.md",
         "entrypoint":"reports.approved_requests.entrypoint.generate",
         "audience":[
            "provider",
            "vendor"
         ],
         "report_spec":"2",
         "parameters":[
            {
               "id":"date",
               "type":"date_range",
               "name":"Report period",
               "description":"Provide the requests creation date period for the report",
               "required":true
            },
            {
               "id":"product",
               "type":"checkbox",
               "name":"Product list",
               "description":"Select the products you want to include in report",
               "required":true,
               "choices": [
                  {
                     "value":"PRD-207-752-513",
                     "label":"Adobe VIP Marketplace"
                  },
                  {
                     "value":"PRD-466-278-670",
                     "label":"Adobe Government"
                  },
                  {
                     "value":"PRD-308-433-016",
                     "label":"Adobe Education"
                  },
                  {
                     "value":"PRD-265-651-368",
                     "label":"Adobe Cloud 2"
                  },
                  {
                     "value":"PRD-450-658-035",
                     "label":"NCE Microsoft Software Subscriptions Commercial"
                  },
                  {
                     "value":"PRD-007-563-386",
                     "label":"NCE Microsoft Perpetual Software Nonprofit"
                  },
                  {
                     "value":"PRD-136-332-888",
                     "label":"NCE Microsoft Perpetual Software Education"
                  },
                  {
                     "value":"PRD-672-364-473",
                     "label":"NCE Microsoft Perpetual Software Commercial"
                  },
                  {
                     "value":"PRD-421-575-828",
                     "label":"Azure NCE DEV"
                  },
                  {
                     "value":"PRD-815-848-109",
                     "label":"Azure RI DEV"
                  }
               ]
            },
            {
               "id":"mkp",
               "type":"marketplace",
               "name":"Marketplaces",
               "required":true,
               "description":"Select the marketplaces you want to include in the report"
            },
            {
               "id":"rr_type",
               "type":"checkbox",
               "name":"Request type",
               "description":"Select the type of the requests you want to include in the report",
               "required":true,
               "choices":[
                  {
                     "value":"purchase",
                     "label":"Purchase"
                  },
                  {
                     "value":"change",
                     "label":"Change"
                  },
                  {
                     "value":"suspend",
                     "label":"Suspend"
                  },
                  {
                     "value":"resume",
                     "label":"Resume"
                  },
                  {
                     "value":"cancel",
                     "label":"Cancel"
                  },
                  {
                     "value":"adjustment",
                     "label":"Adjustment"
                  }
               ]
            },
            {
               "id":"connexion_type",
               "type":"checkbox",
               "name":"Type of Environment",
               "required":true,
               "description":"Select the type of environment you want to include in report",
               "choices":[
                  {
                     "value":"production",
                     "label":"Production"
                  },
                  {
                     "value":"test",
                     "label":"Test"
                  },
                  {
                     "value":"preview",
                     "label":"Preview"
                  }
               ]
            }
         ],
         "renderers":[
            {
               "id":"xlsx",
               "type":"xlsx",
               "default":true,
               "description":"Export data in Microsoft Excel 2020 format.",
               "template":"reports/approved_requests/templates/xlsx/template.xlsx",
               "args":{
                  "start_row":2,
                  "start_col":1
               }
            }
         ]
      }
   ]
}